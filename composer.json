{
  "name": "squarecloudlabs/snowflake-laravel-driver",
  "description": "A Snowflake database driver for Laravel.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Snowflake\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snowflake\\Laravel\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Nigel James",
      "email": "nigel.james@squarecloud.com.au"
    },
    {
      "name": "Aaron Francis",
      "email": "aarondfrancis@gmail.com"
    },
    {
      "name": "Carl Sverre",
      "email": "carl@singlestore.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/container": "^8.0|^9.0|^10.0",
    "illuminate/database": "^8.0|^9.0|^10.0",
    "illuminate/events": "^8.0|^9.0|^10.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^8.5.23|^9.5"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Snowflake\\Laravel\\SnowflakeProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
